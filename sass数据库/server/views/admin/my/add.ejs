<% include ../header %>


    <!-- 引入 富文本编辑器 -->
    <script src="/ueditor/ueditor.config.js"></script>
    <script src="/ueditor/ueditor.all.min.js"></script>

    <!-- 路径导航 -->
    <ol class="breadcrumb">
        <li>
            <a href="#">个人信息管理</a>
        </li>
        <li class="active">添加资料</li>
    </ol>
    <!-- 路径导航 END -->


    <!-- 发布文章 -->
    <div class="container myAdd">
        <div class="col-md-4"></div>
        <div class="col-md-4">
            <form action="/admin/my/doadd" method="POST" enctype="multipart/form-data">
                <div class="col-md-12">
                    <div class="input-group">
                        <span class="input-group-addon">功能编号</span>
                        <input type="text" class="form-control" name="id">
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="input-group">
                        <span class="input-group-addon">功能名称</span>
                        <input type="text" class="form-control" name="name">
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="input-group">
                        <span class="input-group-addon">功能图像</span>
                        <input type="file" class="form-control" name="imgurl">
                    </div>
                </div>
                <div class="col-md-6">
                    <input class="reset" type="reset">
                </div>
                <div class="col-md-6">
                    <input class="submit" type="submit">
                </div>
            </form>
        </div>
        <div class="col-md-4"></div>
    </div>

    <% include ../footer %>